URxvt.background:	#1F1D19
URxvt.foreground:	#DEDEDE
!URxvt.font:			xft:DejaVu Sans Mono:pixelsize=15:antialias=true:hinting=true
!URxvt.font: xft:MesloLGMDZ Nerd Font Mono
!URxvt.font: xft:Meslo LG M DZ for Powerline:pixelsize=18
URxvt.font: xft:agave Nerd Font Mono:pixelsize=22
!URxvt.font: xft:Droid Sans Mono for Powerline:size=10
!URxvt.letterSpace: -2
!URxvt.boldFont:		xft:DejaVu Sans Mono:bold:pixelsize=15:antialias=true:hinting=true
URxvt.scrollBar:	false
URxvt.saveLines:	65535
URxvt.perl-ext-common: keyboard-select,url-select,clipboard,digital-clock
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keysym.M-u: perl:url-select:select_next

! For working with tmux [urxvt -> xterm]

! Disable ISO14755 mode (Ctrl-Shift)
URxvt.iso14755: false
URxvt.iso14755_52: false

! Fix resize pane commands
URxvt.keysym.C-S-Up: string:\033[1;6A
URxvt.keysym.C-S-Down: string:\033[1;6B
URxvt.keysym.C-S-Left: string:\033[1;6D
URxvt.keysym.C-S-Right: string:\033[1;6C

!URxvt.keysym.M-c:   perl:clipboard:copy
!URxvt.keysym.M-v:   perl:clipboard:paste
!URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
Xft*dpi:       96
Xft*antialias: True
Xft*hinting:   Full
URxvt.keysym.KP_Add:    +
URxvt.keysym.KP_Subtract: -
URxvt.keysym.KP_Multiply: *
URxvt.keysym.KP_Divide  : /
URxvt.keysym.KP_Decimal : .
URxvt.keysym.KP_Separator : ,
URxvt.keysym.KP_1:      1
URxvt.keysym.KP_2:      2
URxvt.keysym.KP_3:      3
URxvt.keysym.KP_4:      4
URxvt.keysym.KP_5:      5
URxvt.keysym.KP_6:      6
URxvt.keysym.KP_7:      7
URxvt.keysym.KP_8:      8
URxvt.keysym.KP_9:      9
URxvt.keysym.KP_0:      0

!Real Transparency ( if your systems supports )
URxvt.depth: 32
URxvt.background: rgba:0000/0000/0800/c200

!Fake Transparency ( fake transparency if your system does not support real t.)
!URxvt.transparent: true
!URxvt.shading: 25
